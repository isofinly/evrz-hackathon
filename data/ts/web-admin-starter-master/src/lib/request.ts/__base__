const SERVER_ENDPOINT = process.env.SERVER_ENDPOINT || '';

type ResponseMeta = {
  code: number;
  url: string;
  method: string;
};

export interface SuccessResponse extends ResponseMeta <BODY SuccessResponse>

export interface FailResponse extends ResponseMeta <BODY FailResponse>

export type FormData = {
  file: File;
  name: string;
  meta?: {
    [index: string]: string;
  };
};

export type RequestProps = {
  method?: string;
  url: string;
  body?: object;
  file?: FormData;
};

function attachFormData({ file, name, meta }: FormData) <BODY attachFormData>

export const request = async ({
  url,
  method = 'get',
  body,
  file,
//<REVIEW>По стандартам разработки - использование any стоит избегать. Вместо этого - протипизируйте дженерик</REVIEW>
}: RequestProps): Promise<any> => <BODY request>;