
import { Dispatch, useEffect } from "react"
import { SetStateAction } from "react"
import { useState } from "react"

export const useLocalStorage = <T = string>(
    name: string,
    defaultValue: T,
    parseCallback: (localStorageItem: string, defaultValue: T) => T,
    stringifyCallback: (item: T | null) => string
): [item: T, setItem: Dispatch<SetStateAction<T>>] => <BODY useLocalStorage>

export const parseBoolean = (item: string, defaultValue: boolean) => <BODY parseBoolean>