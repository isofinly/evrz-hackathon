


export type KeyboardShortcutsEvent = {
    key: string
    ctrlKey: boolean
    shiftKey: boolean
    altKey: boolean
    metaKey: boolean
}


export const keyboardShortcuts =
    <T extends KeyboardShortcutsEvent>(shortcuts: Record<string, (e: T) => void>) =>
    (e: T) => <BODY keyboardShortcuts>





export const getIsCurrentSymbol = (e: KeyboardShortcutsEvent, key: string) => <BODY getIsCurrentSymbol>