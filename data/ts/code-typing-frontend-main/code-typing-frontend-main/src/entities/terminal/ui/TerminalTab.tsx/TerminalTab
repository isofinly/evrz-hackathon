TerminalTab = () => {
    const isOpenTerminal = useTerminalIsOpen()
    const { toggleTerminal } = useTerminalHandlers()

    return (
        <Tooltip label="Project Info (Alt + F12)">
            <div>
                <AsideTab isActive={isOpenTerminal} onClick={toggleTerminal}>
                    <TerminalIcon />
                </AsideTab>
            </div>
        </Tooltip>
    )
}