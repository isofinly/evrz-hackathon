DetailsKey = (props: DetailsKeyProps) => {
    const { name, onShowDetails, openDetails, ...restProps } = props

    return (
        <ResultKey
            value={
                <Button variant="outline" size="sm" onClick={onShowDetails}>
                    {openDetails === name ? `Hide ${name} details` : `Show ${name} details`}
                </Button>
            }
            {...restProps}
        />
    )
}