CodeIndexesRange = (props: CodeIndexesRangeProps) => {
    const { length, startIndex, ...codeIndexesProps } = props

    return (
        <CodeIndexes {...codeIndexesProps}>
            {length > 0 &&
                Array.from({ length: length }).map((_, index) => (
                    <CodeIndex key={index} index={index + startIndex} />
                ))}
        </CodeIndexes>
    )
}