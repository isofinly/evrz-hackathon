{"start_line": 10, "code": "Categories = ({ category, setCategory }: Props): JSX.Element => {\n  //<REVIEW>\u0414\u0430\u043d\u043d\u044b\u0439 \u043a\u043e\u043c\u043f\u043e\u043d\u0435\u043d\u0442 \u0432\u043e\u0437\u0432\u0440\u0430\u0449\u0430\u0435\u0442 \u0441\u0442\u0430\u0442\u0438\u0447\u043d\u044b\u0439 \u0441\u043f\u0438\u0441\u043e\u043a \u044d\u043b\u0435\u043c\u0435\u043d\u0442\u043e\u0432 \u0438 \u043e\u0442 \u043d\u0435 \u043c\u0435\u043d\u044f\u0435\u0442\u0441\u044f. \u0421 \u0446\u0435\u043b\u044c\u044e \u0443\u043f\u0440\u043e\u0441\u0442\u0438\u0442\u044c \u043a\u043e\u043c\u043f\u043e\u043d\u0435\u043d\u0442 \u0438 \u043f\u043e\u043d\u0438\u043c\u0430\u043d\u0438\u0435 \u043a\u043e\u0434\u0430 - \u0441\u0442\u043e\u0438\u0442 \u0437\u0430\u043d\u0435\u0441\u0442\u0438 \u0441\u043f\u0438\u0441\u043e\u043a \u0432 \u043a\u043e\u043d\u0441\u0442\u0430\u043d\u0442\u0443 \u0438 \u0440\u0435\u043d\u0434\u0435\u0440\u0438\u0442\u044c \u043a\u043e\u043c\u043f\u043e\u043d\u0435\u043d\u0442 \u043f\u043e \u044d\u0442\u043e\u043c\u0443 \u0441\u043f\u0438\u0441\u043a\u0443.</REVIEW>\n  return (\n    <div className={styles.selectWrapper}>\n      <select\n        className={styles.Select}\n        value={category}\n        onChange={(e) =>\n          setCategory((e.target as HTMLSelectElement).value as Category)\n        }\n      >\n        <option className={styles.option} value={\"all\"}>\n          All\n        </option>\n        <option className={styles.option} value={\"automotive\"}>\n          Automotive\n        </option>\n        <option className={styles.option} value={\"groceries\"}>\n          Groceries\n        </option>\n        <option className={styles.option} value={\"home-decoration\"}>\n          Home decoration\n        </option>\n        <option className={styles.option} value={\"fragrances\"}>\n          Fragrances\n        </option>\n        <option className={styles.option} value={\"furniture\"}>\n          Furniture\n        </option>\n        <option className={styles.option} value={\"laptops\"}>\n          Laptops\n        </option>\n        <option className={styles.option} value={\"lighting\"}>\n          Lighting\n        </option>\n        <option className={styles.option} value={\"mens-shirts\"}>\n          Mens shirts\n        </option>\n        <option className={styles.option} value={\"mens-shoes\"}>\n          Mens shoes\n        </option>\n        <option className={styles.option} value={\"mens-watches\"}>\n          Mens watches\n        </option>\n        <option className={styles.option} value={\"motorcycle\"}>\n          Motorcycle\n        </option>\n        <option className={styles.option} value={\"skincare\"}>\n          Skincare\n        </option>\n        <option className={styles.option} value={\"smartphones\"}>\n          Smartphones\n        </option>\n        <option className={styles.option} value={\"sunglasses\"}>\n          Sunglasses\n        </option>\n        <option className={styles.option} value={\"tops\"}>\n          Tops\n        </option>\n        <option className={styles.option} value={\"womens-bags\"}>\n          Womens bags\n        </option>\n        <option className={styles.option} value={\"womens-dresses\"}>\n          Womens dresses\n        </option>\n        <option className={styles.option} value={\"womens-jewellery\"}>\n          Womens jewellery\n        </option>\n        <option className={styles.option} value={\"womens-shoes\"}>\n          Womens shoes\n        </option>\n        <option className={styles.option} value={\"womens-watches\"}>\n          Womens watches\n        </option>\n      </select>\n    </div>\n  );\n}"}