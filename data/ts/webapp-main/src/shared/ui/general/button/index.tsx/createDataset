createDataset = ({
  loading = false,
  disabled = false,
  size = 'md',
  variant = 'solid',
  color = 'primary',
  fluid = false,
}: SharedProps) => ({
  'data-disabled': loading || disabled,
  'data-loading': loading,
  'data-size': size,
  'data-color': color,
  'data-variant': variant,
  'data-fluid': fluid,
})