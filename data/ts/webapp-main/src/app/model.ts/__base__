import { $$goBack } from '@features/go-back'
import { history, router } from '@routing/shared'
import { createEvent, sample } from 'effector'

export const appStarted = createEvent()



sample({
  clock: appStarted,
  fn: () => history,
  target: router.setHistory,
})



sample({
  clock: appStarted,
  target: $$goBack.setupFx,
})