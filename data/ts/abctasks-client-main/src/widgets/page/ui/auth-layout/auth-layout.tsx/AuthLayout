AuthLayout: React.FC<React.PropsWithChildren<AuthLayoutProps>> = (
	props
) => {
	const { className, classes, children } = props;
	return (
		<Layout
			className={className}
			slots={{
				header: <AuthHeader className={classes?.header} />,
			}}>
			<Center className={classes?.center} height='container'>
				{children}
			</Center>
		</Layout>
	);
}