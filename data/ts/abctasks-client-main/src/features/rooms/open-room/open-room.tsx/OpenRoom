OpenRoom: React.FC<OpenRoomProps> = (props) => {
	const { id, className, } = props;
	const { t, } = useTranslation('common');

	const nameText = t('actions.open');

	return (
		<Button
			className={className}
			variant='contained'
			to={routes.room.tasks as any}
			params={{ id, }}
			component={Link}
			disableElevation>
			{nameText}
		</Button>
	);
}