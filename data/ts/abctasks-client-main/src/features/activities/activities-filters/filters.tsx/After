After: React.FC = () => {
	// <REVIEW>переменная должна иметь явный смысл в своем названии</REVIEW>
	const { t } = useTranslation('common');

	const label = t('fields.create_after');
	const after = useUnit(form.fields.after);

	return (
		<DatePicker
			label={label}
			value={after.value}
			onChange={after.onChange}
			onBlur={after.onBlur}
			helperText={after.errorText}
			isValid={after.isValid}
			name='after'
		/>
	);
}